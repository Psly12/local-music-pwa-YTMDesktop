# Production environment configuration

# Enable/disable logging in production (default: disabled)
VITE_ENABLE_LOGGING=false

# Analytics configuration (optional)
# VITE_GOATCOUNTER_CODE=your_code_here

# YouTube Music Desktop API settings
# Default host for YTM Desktop connection
VITE_YTM_DEFAULT_HOST=127.0.0.1

# Default port for YTM Desktop connection  
VITE_YTM_DEFAULT_PORT=9863

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Build configuration
NODE_ENV=production